{
  "name": "Mi Extensión de Chrome",
  "version": "1.0",
  "manifest_version": 3,
  "description": "Esta es una extensión de ejemplo",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "img/16.png",
      "32": "img/32.png",
      "48": "img/48.png",
      "128": "img/128.png"
    }
  },
  "content_scripts":[
		{
			"matches":[
				"https://chat.openai.com/*"
			],
			"js": ["jquery.min.js", "fontawesome.min.js", "content.js"]
		}
	],
  "permissions": [
    "activeTab"
  ]
}